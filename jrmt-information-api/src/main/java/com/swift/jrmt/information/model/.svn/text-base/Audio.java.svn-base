		this.auditTime = auditTime;
	}

	public java.util.Date getAuditTime(){
		return this.auditTime;
	}


	public void setPubTime(java.util.Date pubTime){
		this.pubTime = pubTime;
	}

	public java.util.Date getPubTime(){
		return this.pubTime;
	}

